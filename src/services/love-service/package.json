{
    "name": "love-service",
    "version": "1.0.0",
    "description": "Love Service for TuneZ Music App",
    "main": "src/index.ts",
    "scripts": {
        "start": "tsx src/index.ts",
        "dev": "npx tsx watch src/index.ts",
        "debug": "node --inspect=0.0.0.0:9227 --nolazy --import tsx src/index.ts",
        "build": "npx",
        "generate-proto": "mkdir -p ./src/grpc/proto ./src/grpc/generated && protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./src/grpc/generated -I ./src/grpc/proto ./src/grpc/proto/*.proto"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@bufbuild/buf": "^1.50.1",
        "@grpc/grpc-js": "^1.13.0",
        "@grpc/proto-loader": "^0.7.13",
        "axios": "^1.8.3",
        "body-parser": "^1.20.3",
        "class-validator": "^0.14.1",
        "compression": "^1.8.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^5.0.1",
        "express-http-proxy": "^2.1.1",
        "firebase-admin": "^13.2.0",
        "http-proxy-middleware": "^3.0.3",
        "multer": "^1.4.5-lts.1",
        "reflect-metadata": "^0.2.2"
    },
    "devDependencies": {
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/express-http-proxy": "^1.6.6",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.13.10",
        "protoc": "^1.1.3",
        "ts-node": "^10.9.2",
        "ts-protoc-gen": "^0.15.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2"
    }
}
