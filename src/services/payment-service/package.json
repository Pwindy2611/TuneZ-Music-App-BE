{
  "name": "payment-service",
  "version": "1.0.0",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "dev": "npx tsx watch src/index.ts",
    "debug": "node --inspect=0.0.0.0:9227 --nolazy --import tsx src/index.ts",
    "build": "npx",
    "generate-proto": "mkdir -p ./src/grpc/proto ./src/grpc/generated && protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./src/grpc/generated -I ./src/grpc/proto ./src/grpc/proto/*.proto"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.13.0",
    "@grpc/proto-loader": "^0.7.13",
    "@types/axios": "^0.9.36",
    "@types/dotenv": "^6.1.1",
    "axios": "^1.8.4",
    "body-parser": "^1.20.3",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.5.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "firebase-admin": "^13.2.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
